{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Make bootstrap",
      "type": "shell",
      "command": "make",
      "args": [
        "bootstrap"
      ],
      "options": {
        "cwd": "${workspaceFolder}/"
      },
      "group": "build"
    },
    {
      "label": "Make clean",
      "type": "shell",
      "command": "make",
      "args": [
        "clean"
      ],
      "options": {
        "cwd": "${workspaceFolder}/"
      },
      "group": "build"
    }
  ]
}
